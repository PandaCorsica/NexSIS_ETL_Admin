$HHMMSS:=Replace string:C233($heure; ":"; "")
$doc:=Create document:C266($cheminDossier+"MOYENS.CSV"; ".csv")
If (OK=1)
	SEND PACKET:C103($doc; "id_moyen"+";")
	SEND PACKET:C103($doc; "nom_moyen"+";")
	SEND PACKET:C103($doc; "type_moyen"+";")
	SEND PACKET:C103($doc; "num_ordre_cis"+";")
	SEND PACKET:C103($doc; "num_ordre_dept"+";")
	SEND PACKET:C103($doc; "immatriculation"+";")
	SEND PACKET:C103($doc; "id_uf_admin"+";")
	SEND PACKET:C103($doc; "id_uf_ope"+";")
	SEND PACKET:C103($doc; "hauteur_max"+";")
	SEND PACKET:C103($doc; "largeur_hors_tout"+";")
	SEND PACKET:C103($doc; "longueur_hors_tout"+";")
	SEND PACKET:C103($doc; "classe_porteur"+";")
	SEND PACKET:C103($doc; "attelage"+";")
	SEND PACKET:C103($doc; "categorie"+";")
	SEND PACKET:C103($doc; "capacite_traction"+";")
	SEND PACKET:C103($doc; "ptac"+";")
	SEND PACKET:C103($doc; "etat_disponibilite;")
	SEND PACKET:C103($doc; "nb_places"+Char:C90(Retour à la ligne:K15:40))
	
	
	QUERY:C277([IMPORT_MOYENS:43]; [IMPORT_MOYENS:43]id_uf_ope:8#"archivage"))
	While (Not:C34(End selection:C36([IMPORT_MOYENS:43])))
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]id_Moyen:2+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]id_Moyen:2+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]vecteur:3+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]num_ordre_cis:4+";")
		SEND PACKET:C103($doc; String:C10([IMPORT_MOYENS:43]num_ordre_dept:5)+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]immatriculation:6+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]id_uf_admin:7+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]id_uf_ope:8+";")
