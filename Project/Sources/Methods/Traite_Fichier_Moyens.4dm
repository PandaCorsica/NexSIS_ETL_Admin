		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]etat_disponibilite:17+";")
		SEND PACKET:C103($doc; ""+Char:C90(Retour à la ligne:K15:40))
		
		
		NEXT RECORD:C51([IMPORT_MOYENS:43])
	End while 
	CLOSE DOCUMENT:C267($doc)
End if 
