		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]classe:12+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]attelage:13+";")
		SEND PACKET:C103($doc; [IMPORT_MOYENS:43]type_vesteur:14+";")
